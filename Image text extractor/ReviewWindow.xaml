<Window x:Class="Image_text_extractor.ReviewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Review" WindowState="Maximized" Background="#FFE4E4E4" Closing="WindowClosing" d:DesignWidth="1158" d:DesignHeight="843">

    <Window.Resources>
        <DataTemplate x:Key="emblemCmbTemplate">
            <WrapPanel Margin="0,0,0,0">
                <Image Width="24" Height="24" Stretch="Fill" Source="{Binding Photo}" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="2,0,15,0"/>
                <Label Content="{Binding Name}" FontSize="12" Margin="0,0,0,0" VerticalAlignment="Center" HorizontalAlignment="Center"/>
            </WrapPanel>
        </DataTemplate>
        <ControlTemplate x:Key="validationTemplate">
            <DockPanel>
                <TextBlock Foreground="Red" FontSize="25" Text="Last Field required" DockPanel.Dock="Right" />
                <AdornedElementPlaceholder/>
            </DockPanel>
        </ControlTemplate>
    </Window.Resources>

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.2*"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="10*"/>
            <RowDefinition Height="0.2*"/>
        </Grid.RowDefinitions>

        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0.3*"/>
            <ColumnDefinition Width="10*"/>
            <ColumnDefinition Width="0.3*"/>
        </Grid.ColumnDefinitions>

        <Border Grid.Row="1" Grid.Column="1" BorderBrush="Black" BorderThickness="1">
            <Grid Grid.Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1.5*"/>
                    <ColumnDefinition Width="4*"/>
                    <ColumnDefinition Width="1.5*"/>
                    <ColumnDefinition Width="4*"/>
                </Grid.ColumnDefinitions>

                <Border  Grid.Column="0" BorderBrush="Black" BorderThickness="0.2">
                    <Grid Grid.Column="0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="2.5*"/>
                            <ColumnDefinition Width="2*"/>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="2*"/>
                        </Grid.ColumnDefinitions>
                        <Label Grid.Column="0" Content="Record" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14"/>
                        <TextBox Grid.Column="1" Text="{Binding PageIndex}" IsReadOnly="True" CharacterCasing="Upper" FontSize="14" Height="18"/>
                        <Label Grid.Column="2" Content="of" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14"/>
                        <TextBox Grid.Column="3" CharacterCasing="Upper" FontSize="14" Height="18" Margin="0,0,5,0"/>
                    </Grid>
                </Border>

                <Border Grid.Column="1" BorderBrush="Black" BorderThickness="0.2">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="1*"/>
                        </Grid.ColumnDefinitions>
                        <TextBox  IsReadOnly="True" CharacterCasing="Upper" FontSize="14" Width="auto" Height="18" Margin="2"/>
                        <TextBox  Grid.Column="1" IsReadOnly="True" CharacterCasing="Upper" FontSize="14" Width="auto" Height="18" Margin="2,2,0,2"/>
                    </Grid>
                </Border>

                <Border Grid.Column="2" BorderBrush="Black" BorderThickness="0.2">
                        <Grid Grid.Column="3">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1*"/>
                                <ColumnDefinition Width="1.2*"/>
                            </Grid.ColumnDefinitions>
                            <Button Height="20" FontSize="12" Margin="0,0,3,0"
                            Grid.Column="1" Click="GoToRecordClick">
                                <TextBlock TextWrapping="Wrap" TextAlignment="Center">
                            Go to Record
                                </TextBlock>
                            </Button>
                            <TextBox Grid.Column="0" Name="GoToRecordTextBox" CharacterCasing="Upper" 
                                     FontSize="14" Height="18" Margin="3,6,5,0" VerticalAlignment="Top"/>
                        </Grid>
                </Border>
                
                <Grid Grid.Column="4">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="0.6*"/>
                        <ColumnDefinition Width="0.6*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                    </Grid.ColumnDefinitions>
                    <Button Height="20" FontSize="12"
                            Grid.Column="0" Click="PreviousClick">
                        <TextBlock TextWrapping="Wrap" TextAlignment="Center">
                             Previous
                        </TextBlock>
                    </Button>
                    <Button Height="20" FontSize="12" 
                            Grid.Column="1" Click="NextClick">
                        <TextBlock TextWrapping="Wrap" TextAlignment="Center">
                            Next
                        </TextBlock>
                    </Button>
                    <Button Height="20" FontSize="12" 
                            Grid.Column="2" Click="PreviousClick">
                        <TextBlock TextWrapping="Wrap" TextAlignment="Center">
                            First Record
                        </TextBlock>
                    </Button>
                    <Button Height="20"  FontSize="12" 
                            Grid.Column="3" Click="NextClick">
                        <TextBlock TextWrapping="Wrap" TextAlignment="Center">
                            Last Record
                        </TextBlock>
                    </Button>
                    <Button Height="20" FontSize="12"
                            Grid.Column="4" Click="BackClick">
                        <TextBlock TextWrapping="Wrap" TextAlignment="Center">
                            Open Image
                        </TextBlock>
                    </Button>
                    <Button Height="20" FontSize="12"
                            Grid.Column="5" Click="BackClick">
                        <TextBlock TextWrapping="Wrap" TextAlignment="Center">
                            Back to Main Menu
                        </TextBlock>
                    </Button>
                </Grid>
            </Grid>
        </Border>

        <Border  Grid.Row="2" Grid.Column="1" BorderBrush="Black" BorderThickness="1">
            <Grid>
                <Grid.Resources>
                    <Style TargetType="TextBox">
                        <Setter Property="CharacterCasing" Value="Upper" />
                        <Setter Property="FontSize" Value="14" />
                    </Style>
                    <Style TargetType="Label">
                        <Setter Property="FontSize" Value="14" />
                    </Style>
                    <Style TargetType="ComboBox">
                        <Setter Property="FontSize" Value="12" />
                        <Setter Property="Height" Value="25" />
                    </Style>
                </Grid.Resources>

                <Grid.RowDefinitions>
                    <RowDefinition Height="1*"/>
                    <RowDefinition Height="1*"/>
                    <RowDefinition Height="1*"/>
                </Grid.RowDefinitions>
                <Grid Grid.Row="0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                    </Grid.ColumnDefinitions>
                    <Label Grid.Column="0" Content="Cemetry Name"  VerticalAlignment="Center"/>
                    <TextBox Grid.Column="0" Text="{Binding CurrentPageData.CemeteryName}"  Margin="120,10,20,10" Padding="2"/>
                    <Label Grid.Column="1" Content="Burial Section #" VerticalAlignment="Center"/>
                    <TextBox Grid.Column="1" Text="{Binding CurrentPageData.BurialSectionNumber}" Margin="120,10,0,10" Padding="2"/>
                </Grid>
                <Grid Grid.Row="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="361*"/>
                        <ColumnDefinition Width="361*"/>
                        <ColumnDefinition Width="79*"/>
                        <ColumnDefinition Width="282*"/>
                    </Grid.ColumnDefinitions>
                    <Label Grid.Column="0" Content="Wall ID #"  VerticalAlignment="Center" Margin="0,8"/>
                    <TextBox  Grid.Column="0" Text="{Binding CurrentPageData.WallID}" Margin="100,10,10,10" Padding="2"/>
                    <Label Grid.Column="1"  Content="Row #" VerticalAlignment="Center" Margin="0,8"/>
                    <TextBox Grid.Column="1" Text="{Binding CurrentPageData.RowNum}" Margin="80,10,10,10" Padding="2"/>
                    <Label Grid.Column="2" Content="Gravesite #"  VerticalAlignment="Center" Grid.ColumnSpan="2" Margin="0,8"/>
                    <TextBox Grid.Column="3" Text="{Binding CurrentPageData.GavestoneNumber}" Margin="11,10,0,10" Padding="2"/>
                </Grid>
                <Grid Grid.Row="2">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="361"/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <Label Grid.Column="0" Content="Marker Type"  VerticalAlignment="Center"/>
                    <ComboBox Grid.Column="0" Text="{Binding CurrentPageData.MarkerType}" Margin="100,10,10,10" Padding="2"
                              HorizontalAlignment="Stretch" VerticalAlignment="Center">
                        <ComboBoxItem Content="Upright Headstone"></ComboBoxItem>
                        <ComboBoxItem Content="Flat Marker"></ComboBoxItem>
                        <ComboBoxItem Content="Niche Cover"></ComboBoxItem>
                        <ComboBoxItem Content="Section Monument"></ComboBoxItem>
                        <ComboBoxItem Content="Temprory Marker"></ComboBoxItem>
                    </ComboBox>
                    <Label Grid.Column="1"  Content="Emblem 1"  VerticalAlignment="Center"/>
                    <ComboBox Grid.Column="1" Text="{Binding CurrentPageData.Emblem1}" Margin="80,10,10,10" Padding="1"
                              Background="White" HorizontalAlignment="Stretch" ItemsSource="{Binding GetEmblemData}"
                              ItemTemplate="{StaticResource emblemCmbTemplate}" VerticalAlignment="Center"/>
                    <Label Grid.Column="2" Content="Emblem 2"  VerticalAlignment="Center"/>
                    <ComboBox Grid.Column="2" Text="{Binding CurrentPageData.Emblem2}"  Margin="90,10,0,10" Padding="1"
                              ItemTemplate="{StaticResource emblemCmbTemplate}" ItemsSource="{Binding GetEmblemData}"
                              HorizontalAlignment="Stretch" VerticalAlignment="Center"/>
                </Grid>
            </Grid>
        </Border>



        <Border Grid.Row="3" Grid.Column="1" BorderBrush="Black" BorderThickness="1">
            <ScrollViewer VerticalScrollBarVisibility="Visible" HorizontalScrollBarVisibility="Visible">
                <Grid Margin="30,0,0,30">
                    <Grid.Resources>
                        <Style TargetType="TextBox">
                            <Setter Property="CharacterCasing" Value="Upper" />
                            <Setter Property="FontSize" Value="16" />
                        </Style>
                        <Style TargetType="Label">
                            <Setter Property="FontSize" Value="14" />
                        </Style>
                        <Style TargetType="ComboBox">
                            <Setter Property="FontSize" Value="16" />
                        </Style>
                    </Grid.Resources>

                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>

                    <Grid Grid.Row="0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>


                        <StackPanel>
                            <Label
                                Margin="0,25,0,0"
                                Content="PRIMARY"
                                FontSize="17"
                                FontWeight="Bold"/>
                            <StackPanel Orientation="Horizontal">
                                <StackPanel>
                                    <Label Content="Name: First" />
                                    <TextBox Text="{Binding CurrentPageData.PrimaryDecedent.FirstName}" 
                                             Margin="2,0,0,2" Padding="1" Width="200" />
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Middle" />
                                    <TextBox Text="{Binding CurrentPageData.PrimaryDecedent.MiddleName}" 
                                             Margin="2,0,0,2" Width="200" />
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Last" />
                                    <TextBox  Text="{Binding CurrentPageData.PrimaryDecedent.LastName}" 
                                              Margin="2,0,0,2" Width="200" />
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Suffix" />
                                    <TextBox Text="{Binding CurrentPageData.PrimaryDecedent.Suffix}" 
                                             Margin="2,0,0,2"  Width="100" />
                                </StackPanel>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <StackPanel>
                                    <Label Content="Location" />
                                    <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.Location}" 
                                              Margin="2,0,0,2" Width="300" Padding="1" 
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center"
                                              ItemsSource="{Binding GetLocation}">
                                        <ComboBox.ItemTemplate>
                                            <DataTemplate>
                                                <DataGridRow DataContext="{Binding}" Height="30" Width="250" 
                                                             HorizontalAlignment="Stretch">
                                                    <DataGridRow.Template>
                                                        <ControlTemplate>
                                                            <Grid>
                                                                <Grid.ColumnDefinitions>
                                                                    <ColumnDefinition Width="5*"/>
                                                                    <ColumnDefinition Width="5"/>
                                                                    <ColumnDefinition Width="1*"/>
                                                                </Grid.ColumnDefinitions>
                                                                <TextBlock Text="{Binding Name}"></TextBlock>
                                                                <Border BorderBrush="Black" BorderThickness="2" Margin="2" Grid.Column="1"></Border>
                                                                <TextBlock Grid.Column="2" Text="{Binding Abbr}"></TextBlock>
                                                            </Grid>
                                                        </ControlTemplate>
                                                    </DataGridRow.Template>
                                                </DataGridRow>
                                            </DataTemplate>
                                        </ComboBox.ItemTemplate>
                                    </ComboBox>
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Rank" />
                                    <TextBox Text="{Binding CurrentPageData.PrimaryDecedent.Rank}"
                                             Margin="2,0,0,2" Width="200"/>
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Branch" />
                                    <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.BranchList[0]}" 
                                              Margin="2,0,0,2" Width="200" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemsSource="{Binding GetLocation}"/>
                                </StackPanel>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <StackPanel>
                                    <Label Content="Branch-Unit Custom" />
                                    <TextBox Text="{Binding CurrentPageData.PrimaryDecedent.BranchUnitCustom}"  
                                             Margin="2,0,0,2"  Width="702"/>
                                </StackPanel>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <StackPanel>
                                    <Label Content="War" />
                                    <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.WarList[0]}" 
                                              Margin="2,0,0,2" Width="100" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemsSource="{Binding GetLocation}"/>
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="War 2" />
                                    <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.WarList[1]}" 
                                              Margin="2,0,0,2" Width="100" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemsSource="{Binding GetLocation}"/>
                                </StackPanel>
                                <StackPanel>
                                    <Label Width="90"/>
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Birth Date" />
                                    <TextBox  VerticalAlignment="Center" Margin="2,0,2,0" Width="200" 
                                            Text="{Binding CurrentPageData.PrimaryDecedent.BirthDate}" />
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Death Date" />
                                    <TextBox Margin="2,0,2,0"  Width="200" PreviewTextInput="NumberValidationTextBox"
                                        Text="{Binding CurrentPageData.PrimaryDecedent.DeathDate}"/>
                                </StackPanel>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <StackPanel>
                                    <Label Content="Award" />
                                    <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.AwardList[0]}" 
                                              Margin="2,0,0,2" Width="100" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemsSource="{Binding GetLocation}"/>
                                </StackPanel>
                                <StackPanel Grid.Column="1">
                                    <Label Content="Award 2" />
                                    <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.AwardList[1]}" 
                                              Margin="2,0,0,2" Width="100" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemsSource="{Binding GetLocation}"/>
                                </StackPanel>
                                <StackPanel>
                                    <Label Width="90"/>
                                </StackPanel>
                                <StackPanel Grid.Column="3">
                                    <Label Content="Awards Custom" />
                                    <TextBox Text="{Binding CurrentPageData.PrimaryDecedent.AwardCustom}" 
                                             Margin="2,0,2,0"  Width="405"/>
                                </StackPanel>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <StackPanel>
                                    <Label Content="Inscription" />
                                    <TextBox Text="{Binding CurrentPageData.PrimaryDecedent.Inscription}"
                                             Margin="2,0,0,2"  Width="700"/>
                                </StackPanel>
                            </StackPanel>
                        </StackPanel>

                        <StackPanel Grid.Column="1" Margin="50,50,0,0">
                            <StackPanel.Resources>
                                <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
                            </StackPanel.Resources>

                            <CheckBox  Name="morePrimaryData"
                                  Content="Add Additional Information for Primary"/>

                            <StackPanel Orientation="Horizontal" 
                                    Visibility="{Binding ElementName=morePrimaryData, Path=IsChecked, 
                                    Converter={StaticResource BooleanToVisibilityConverter}}">
                                <StackPanel>
                                    <Label Content="War 3" />
                                    <ComboBox  Text="{Binding CurrentPageData.PrimaryDecedent.WarList[2]}" 
                                               Margin="2,0,0,2" Width="100" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemsSource="{Binding GetLocation}"/>
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="War 4" />
                                    <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.WarList[3]}" 
                                              Margin="2,0,0,2" Width="100" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemsSource="{Binding GetLocation}"/>
                                </StackPanel>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" 
                                    Visibility="{Binding ElementName=morePrimaryData, Path=IsChecked, 
                                    Converter={StaticResource BooleanToVisibilityConverter}}">
                                <StackPanel>
                                    <Label Content="Award 3" />
                                    <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.AwardList[2]}" 
                                              Margin="2,0,0,2" Width="90" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemsSource="{Binding GetLocation}"/>
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Award 4" />
                                    <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.AwardList[3]}" 
                                              Margin="2,0,0,2" Width="90" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemsSource="{Binding GetLocation}"/>
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Award 5" />
                                    <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.AwardList[4]}"  
                                              Margin="2,0,0,2" Width="90" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemsSource="{Binding GetLocation}"/>
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Award 6" />
                                    <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.AwardList[5]}" 
                                              Margin="2,0,0,2" Width="90" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemsSource="{Binding GetLocation}"/>
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Award 7" />
                                    <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.AwardList[6]}" 
                                              Margin="2,0,0,2" Width="90" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemsSource="{Binding GetLocation}"/>
                                </StackPanel>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" 
                                    Visibility="{Binding ElementName=morePrimaryData, Path=IsChecked, 
                                    Converter={StaticResource BooleanToVisibilityConverter}}">
                                <StackPanel>
                                    <Label Content="Rank 2" />
                                    <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.RankList[1]}"  
                                              Margin="2,0,0,2" Width="100" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemsSource="{Binding GetLocation}"/>
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Branch 2" />
                                    <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.BranchList[1]}"
                                              Margin="2,0,0,2" Width="100" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemsSource="{Binding GetLocation}"/>
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Rank 3" />
                                    <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.RankList[2]}"
                                              Margin="2,0,0,2" Width="100" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemsSource="{Binding GetLocation}"/>
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Branch 3" />
                                    <ComboBox  Text="{Binding CurrentPageData.PrimaryDecedent.RankList[2]}"
                                               Margin="2,0,0,2" Width="100" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemsSource="{Binding GetLocation}"/>
                                </StackPanel>
                            </StackPanel>
                        </StackPanel>
                    </Grid>


                    <Grid Grid.Row="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <StackPanel>
                            <Label
                                Margin="0,50,0,0"
                                Content="SECONDARY"
                                FontSize="17"
                                FontWeight="Bold"/>
                            <StackPanel Orientation="Horizontal">
                                <StackPanel>
                                    <Label Content="Name: First" />
                                    <TextBox Text="{Binding CurrentPageData.OthersDecedentList[0].FirstName}" 
                                             Margin="2,0,0,2" Padding="1" Width="200" />
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Middle" />
                                    <TextBox Text="{Binding CurrentPageData.OthersDecedentList[0].MiddleName}"
                                             Margin="2,0,0,2" Width="200" />
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Last" />
                                    <TextBox Text="{Binding CurrentPageData.OthersDecedentList[0].LastName}"
                                             Margin="2,0,0,2" Width="200" />
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Suffix" />
                                    <TextBox Text="{Binding CurrentPageData.OthersDecedentList[0].Suffix}"
                                             Margin="2,0,0,2"  Width="100" />
                                </StackPanel>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <StackPanel>
                                    <Label Content="Location" />
                                    <ComboBox Text="{Binding CurrentPageData.OthersDecedentList[0].Location}"
                                              Margin="2,0,0,2" Width="300" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemsSource="{Binding GetLocation}"/>
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Rank" />
                                    <TextBox Text="{Binding CurrentPageData.OthersDecedentList[0].RankList[0]}"
                                             Margin="2,0,0,2" Width="200" />
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Branch" />
                                    <ComboBox Text="{Binding CurrentPageData.OthersDecedentList[0].BranchList[0]}"
                                              Margin="2,0,0,2" Width="200" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemsSource="{Binding GetLocation}"/>
                                </StackPanel>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <StackPanel>
                                    <Label Content="Branch-Unit Custom" />
                                    <TextBox Text="{Binding CurrentPageData.OthersDecedentList[0].BranchCustom}"
                                             Margin="2,0,0,2"  Width="702"/>
                                </StackPanel>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <StackPanel>
                                    <Label Content="War" />
                                    <ComboBox Text="{Binding CurrentPageData.OthersDecedentList[0].WarList[0]}"
                                              Margin="2,0,0,2" Width="100" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemsSource="{Binding GetLocation}"/>
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="War 2" />
                                    <ComboBox Text="{Binding CurrentPageData.OthersDecedentList[0].WarList[1]}"
                                              Margin="2,0,0,2" Width="100" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemsSource="{Binding GetLocation}"/>
                                </StackPanel>
                                <StackPanel>
                                    <Label Width="90"/>
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Birth Date" />
                                    <TextBox Text="{Binding CurrentPageData.OthersDecedentList[0].BirthDate}"
                                             Margin="2,0,2,0"  Width="200"/>
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Death Date" />
                                    <TextBox Text="{Binding CurrentPageData.OthersDecedentList[0].DeathDate}"
                                             Margin="2,0,2,0"  Width="200"/>
                                </StackPanel>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <StackPanel>
                                    <Label Content="Award" />
                                    <ComboBox Text="{Binding CurrentPageData.OthersDecedentList[0].AwardList[0]}"
                                              Margin="2,0,0,2" Width="100" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemsSource="{Binding GetLocation}"/>
                                </StackPanel>
                                <StackPanel Grid.Column="1">
                                    <Label Content="Award 2" />
                                    <ComboBox Text="{Binding CurrentPageData.OthersDecedentList[0].AwardList[1]}"
                                              Margin="2,0,0,2" Width="100" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemsSource="{Binding GetLocation}"/>
                                </StackPanel>
                                <StackPanel>
                                    <Label Width="90"/>
                                </StackPanel>
                                <StackPanel Grid.Column="3">
                                    <Label Content="Awards Custom" />
                                    <TextBox Text="{Binding CurrentPageData.OthersDecedentList[0].AwardCustom}"
                                             Margin="2,0,2,0"  Width="405"/>
                                </StackPanel>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <StackPanel>
                                    <Label Content="Inscription" />
                                    <TextBox Text="{Binding CurrentPageData.OthersDecedentList[0].Inscription}"
                                             Margin="2,0,0,2"  Width="700"/>
                                </StackPanel>
                            </StackPanel>
                        </StackPanel>


