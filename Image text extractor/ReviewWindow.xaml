<Window x:Class="Image_text_extractor.ReviewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        xmlns:s="clr-namespace:System;assembly=mscorlib"
        mc:Ignorable="d"
        Title="Review" WindowState="Maximized" Background="#FFE4E4E4" KeyDown="Window_KeyDown"
        Closing="WindowClosing" d:DesignWidth="1158" d:DesignHeight="843">

    <Window.Resources>
        <DataTemplate x:Key="emblemCmbTemplate">
            <WrapPanel Margin="0,0,0,0">
                <Image Width="24" Height="24" Stretch="Fill" Source="{Binding Photo}" 
                       VerticalAlignment="Center" HorizontalAlignment="Center" Margin="2,0,15,0"/>
                <Label Content="{Binding Code}" FontSize="14" Margin="0,0,0,0"  Width="50"
                       VerticalAlignment="Center" HorizontalAlignment="Center"/>
                <Label Content="{Binding Name}" FontSize="14" Margin="0,0,0,0" Width="330"
                       VerticalAlignment="Center" HorizontalAlignment="Center"/>
            </WrapPanel>
        </DataTemplate>

        <DataTemplate x:Key="cemeteryNamesCmbTemplate">
            <WrapPanel Margin="0,0,0,0">
                <Label Content="{Binding CemeteryName}" FontSize="14" Margin="0,0,0,0" 
                       Width="400"
                       VerticalAlignment="Center" HorizontalAlignment="Center"/>
                <Label Content="{Binding KeyName}" FontSize="14" Margin="0,0,0,0" Width="50"
                       VerticalAlignment="Center" HorizontalAlignment="Center"/>
            </WrapPanel>
        </DataTemplate>

        <DataTemplate x:Key="locationCmbTemplate">
            <WrapPanel Margin="0,0,0,0">
                <Label Content="{Binding Location}" FontSize="14" Margin="0,0,0,0" Width="230"
                       VerticalAlignment="Center" HorizontalAlignment="Center"/>
                <Label Content="{Binding Abbr}" FontSize="14" Margin="0,0,0,0" Width="50"
                       VerticalAlignment="Center" HorizontalAlignment="Center"/>
            </WrapPanel>
        </DataTemplate>

        <DataTemplate x:Key="branchCmbTemplate">
            <WrapPanel Margin="0,0,0,0">
                <Label Content="{Binding Code}" FontSize="14" Margin="0,0,0,0" Width="50"
                       VerticalAlignment="Center" HorizontalAlignment="Center"/>
                <Label Content="{Binding ShortDescription}" FontSize="14" Margin="0,0,0,0" Width="Auto"
                       VerticalAlignment="Center" HorizontalAlignment="Center"/>
            </WrapPanel>
        </DataTemplate>

        <DataTemplate x:Key="warCmbTemplate">
            <WrapPanel Margin="0,0,0,0">
                <Label Content="{Binding Code}" FontSize="14" Margin="0,0,0,0" Width="50"
                       VerticalAlignment="Center" HorizontalAlignment="Center"/>
                <Label Content="{Binding ShortDescription}" FontSize="14" Margin="0,0,0,0" Width="Auto"
                       VerticalAlignment="Center" HorizontalAlignment="Center"/>
            </WrapPanel>
        </DataTemplate>

        <DataTemplate x:Key="awardCmbTemplate">
            <WrapPanel Margin="0,0,0,0">
                <Label Content="{Binding Code}" FontSize="14" Margin="0,0,0,0" Width="50"
                       VerticalAlignment="Center" HorizontalAlignment="Center"/>
                <Label Content="{Binding Award}" FontSize="14" Margin="0,0,0,0" Width="Auto"
                       VerticalAlignment="Center" HorizontalAlignment="Center"/>
            </WrapPanel>
        </DataTemplate>

        <Style x:Key="ValidationTextBox" TargetType="{x:Type TextBox}">
            <Setter Property="Validation.ErrorTemplate">
                <Setter.Value>
                    <ControlTemplate>
                        <Border BorderBrush="Red" BorderThickness="1">
                            <AdornedElementPlaceholder />
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </Window.Resources>

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.2*"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="10*"/>
            <RowDefinition Height="0.2*"/>
        </Grid.RowDefinitions>

        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0.3*"/>
            <ColumnDefinition Width="10*"/>
            <ColumnDefinition Width="0.3*"/>
        </Grid.ColumnDefinitions>

        <Border Grid.Row="1" Grid.Column="1" BorderBrush="Black" BorderThickness="1">
            <Grid Grid.Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1.5*"/>
                    <ColumnDefinition Width="3*"/>
                    <ColumnDefinition Width="1.5*"/>
                    <ColumnDefinition Width="4*"/>
                </Grid.ColumnDefinitions>

                <Border  Grid.Column="0" BorderBrush="Black" BorderThickness="0.2">
                    <Grid Grid.Column="0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="2.5*"/>
                            <ColumnDefinition Width="2.5*"/>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="2.5*"/>
                        </Grid.ColumnDefinitions>
                        <Label Grid.Column="0" Content="Record" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14"/>
                        <TextBox Grid.Column="1" Text="{Binding PageIndex}" IsReadOnly="True" IsEnabled="False" CharacterCasing="Upper" FontSize="14" Height="20"/>
                        <Label Grid.Column="2" Content="of" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14"/>
                        <TextBox Grid.Column="3" Text="{Binding GetDatabaseCount, Mode=OneWay}" CharacterCasing="Upper" IsReadOnly="True" IsEnabled="False" FontSize="14" Height="20" Margin="0,0,5,0"/>
                    </Grid>
                </Border>

                <Border Grid.Column="1" BorderBrush="Black" BorderThickness="0.2">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="1*"/>
                        </Grid.ColumnDefinitions>
                        <TextBox  Text="{Binding CurrentPageData.SequenceID}" IsEnabled="True"
                                  IsReadOnly="True" CharacterCasing="Upper" FontSize="14" Width="auto" Height="20" Margin="2"/>
                        <TextBox  Text="{Binding CurrentPageData.PrimaryKey}" IsEnabled="True" Margin="2,2,2,2"
                                  Grid.Column="1" IsReadOnly="True" CharacterCasing="Upper" FontSize="14" Width="auto" Height="20"/>
                    </Grid>
                </Border>

                <Border Grid.Column="2" BorderBrush="Black" BorderThickness="0.2">
                    <Grid Grid.Column="3">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="1.2*"/>
                        </Grid.ColumnDefinitions>
                        <TextBox Grid.Column="0" Name="GoToRecordTextBox" CharacterCasing="Upper" 
                                     FontSize="14" Height="20" Margin="2"/>
                        <Button Height="20" FontSize="12" Margin="0,0,3,0"
                            Grid.Column="1" Click="GoToRecordClick">
                            <TextBlock TextWrapping="Wrap" TextAlignment="Center">
                            Go to Record
                            </TextBlock>
                        </Button>
                    </Grid>
                </Border>

                <Grid Grid.Column="4">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="0.6*"/>
                        <ColumnDefinition Width="0.6*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                    </Grid.ColumnDefinitions>
                    <Button Height="20" FontSize="12" 
                            Grid.Column="0" Click="PreviousClick">
                        <TextBlock TextWrapping="Wrap" TextAlignment="Center">
                             Previous
                        </TextBlock>
                    </Button>
                    <Button Height="20" FontSize="12" 
                            Grid.Column="1" Click="NextClick">
                        <TextBlock TextWrapping="Wrap" TextAlignment="Center">
                            Next
                        </TextBlock>
                    </Button>
                    <Button Height="20" FontSize="12" 
                            Grid.Column="2" Click="FirstRecordClick">
                        <TextBlock TextWrapping="Wrap" TextAlignment="Center">
                            First Record
                        </TextBlock>
                    </Button>
                    <Button Height="20"  FontSize="12" 
                            Grid.Column="3" Click="LastRecordClick">
                        <TextBlock TextWrapping="Wrap" TextAlignment="Center">
                            Last Record
                        </TextBlock>
                    </Button>
                    <Button Height="20" FontSize="12"
                            Grid.Column="4" Click="OpenImageClick">
                        <TextBlock TextWrapping="Wrap" TextAlignment="Center">
                            Open Image
                        </TextBlock>
                    </Button>
                    <Button Height="20" FontSize="12"
                            Grid.Column="5" Click="BackClick">
                        <TextBlock TextWrapping="Wrap" TextAlignment="Center">
                            Back to Main Menu
                        </TextBlock>
                    </Button>
                </Grid>
            </Grid>
        </Border>

        <Border  Grid.Row="2" Grid.Column="1" BorderBrush="Black" BorderThickness="1">
            <Grid>
                <Grid.Resources>
                    <Style TargetType="TextBox">
                        <Setter Property="CharacterCasing" Value="Upper" />
                        <Setter Property="FontSize" Value="14" />
                    </Style>
                    <Style TargetType="Label">
                        <Setter Property="FontSize" Value="14" />
                    </Style>
                    <Style TargetType="ComboBox">
                        <Setter Property="FontSize" Value="14" />
                        <Setter Property="Height" Value="25" />
                    </Style>
                </Grid.Resources>

                <Grid.RowDefinitions>
                    <RowDefinition Height="1*"/>
                    <RowDefinition Height="1*"/>
                    <RowDefinition Height="1*"/>
                </Grid.RowDefinitions>
                <Grid Grid.Row="0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="127*"/>
                        <ColumnDefinition Width="415*"/>
                        <ColumnDefinition Width="541*"/>
                    </Grid.ColumnDefinitions>
                    <Label Grid.Column="0" Content="Cemetry Name"  VerticalAlignment="Center" Grid.ColumnSpan="2" Margin="0,8"/>
                    <ComboBox Grid.Column="0" Text="{Binding CurrentPageData.CemeteryName}"
                              HorizontalAlignment="Stretch" ItemsSource="{Binding GetCemeteryNames}"
                              IsEditable="True" IsTextSearchEnabled="True" TextSearch.TextPath="CemeteryName" 
                              ItemTemplate="{StaticResource cemeteryNamesCmbTemplate}" LostFocus="Cemetery_LostFocus"
                              VerticalAlignment="Center" Margin="120,9,50,11" Padding="1" Grid.ColumnSpan="2"/>
                    <Label Grid.Column="2" Content="Burial Section #" VerticalAlignment="Center" Margin="0,8"/>
                    <TextBox x:Name="BurialSectionField" Grid.Column="2" Text="{Binding CurrentPageData.BurialSectionNumber}" Margin="120,10,0,10" Padding="2"/>
                </Grid>
                <Grid Grid.Row="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="361*"/>
                        <ColumnDefinition Width="361*"/>
                        <ColumnDefinition Width="79*"/>
                        <ColumnDefinition Width="282*"/>
                    </Grid.ColumnDefinitions>
                    <Label Grid.Column="0" Content="Wall ID #"  VerticalAlignment="Center" Margin="0,8"/>
                    <TextBox  Grid.Column="0" Text="{Binding CurrentPageData.WallID}" Margin="100,10,10,10" Padding="2"/>
                    <Label Grid.Column="1"  Content="Row #" VerticalAlignment="Center" Margin="0,8"/>
                    <TextBox Grid.Column="1" Text="{Binding CurrentPageData.RowNum}" Margin="80,10,10,10" Padding="2"/>
                    <Label Grid.Column="2" Content="Gravesite #"  VerticalAlignment="Center" Grid.ColumnSpan="2" Margin="0,8"/>
                    <TextBox x:Name="gravesiteNum" Grid.Column="3" Text="{Binding CurrentPageData.GavestoneNumber}" Margin="11,10,0,10" Padding="2"/>
                </Grid>
                <Grid Grid.Row="2">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="361"/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <Label Grid.Column="0" Content="Marker Type"  VerticalAlignment="Center"/>
                    <ComboBox Grid.Column="0" Text="{Binding CurrentPageData.MarkerType}" Margin="100,10,10,10" 
                              Padding="2" HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                              IsEditable="True" IsTextSearchEnabled="True" LostFocus="MarkerCombox_LostFocus">
                        <ComboBoxItem Content="Upright Headstone"></ComboBoxItem>
                        <ComboBoxItem Content="Flat Marker"></ComboBoxItem>
                        <ComboBoxItem Content="Niche Cover"></ComboBoxItem>
                        <ComboBoxItem Content="Section Monument"></ComboBoxItem>
                        <ComboBoxItem Content="Temprory Marker"></ComboBoxItem>
                    </ComboBox>
                    <Label Grid.Column="1"  Content="Emblem 1"  VerticalAlignment="Center"/>
                    <ComboBox Grid.Column="1" Margin="80,10,10,10" Padding="1" 
                              HorizontalAlignment="Stretch" ItemsSource="{Binding GetEmblemData}"
                              ItemTemplate="{StaticResource emblemCmbTemplate}" IsEditable="True"
                              Text="{Binding CurrentPageData.Emblem1}" LostFocus="EmblemCombox_LostFocus"
                              TextSearch.TextPath="Code" IsTextSearchEnabled="True"
                              VerticalAlignment="Center"/>
                    <Label Grid.Column="2" Content="Emblem 2"  VerticalAlignment="Center"/>
                    <ComboBox Grid.Column="2"  Margin="90,10,0,10" Padding="1" 
                              HorizontalAlignment="Stretch" ItemsSource="{Binding GetEmblemData}"
                              ItemTemplate="{StaticResource emblemCmbTemplate}" IsEditable="True"
                              Text="{Binding CurrentPageData.Emblem2}"  LostFocus="EmblemCombox_LostFocus"
                              TextSearch.TextPath="Code" IsTextSearchEnabled="True"
                              VerticalAlignment="Center"/>
                </Grid>
            </Grid>
        </Border>
      

        <Border Grid.Row="3" Grid.Column="1" BorderBrush="Black" BorderThickness="1">
            <Grid>
                <Grid.Resources>
                    <Style TargetType="TextBox">
                        <Setter Property="CharacterCasing" Value="Upper" />
                        <Setter Property="FontSize" Value="16" />
                    </Style>
                    <Style TargetType="Label">
                        <Setter Property="FontSize" Value="14" />
                    </Style>
                    <Style TargetType="ComboBox">
                        <Setter Property="FontSize" Value="16" />
                        <Setter Property="IsEditable" Value="True"/>
                        <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Visible"/>
                    </Style>
                </Grid.Resources>

                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="1*"/>
                </Grid.ColumnDefinitions>

                <Grid Grid.Column="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="auto"/>
                        <ColumnDefinition Width="auto"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>

                    <Image x:Name="frontFaceImage" Grid.Column="1" HorizontalAlignment="Right"/>
                    <Image x:Name="backFaceImage" Grid.Column="2" HorizontalAlignment="Center"/>
                </Grid>

                <ScrollViewer VerticalScrollBarVisibility="Visible" HorizontalScrollBarVisibility="Visible">

                    <Border BorderBrush="Black" BorderThickness="1">
                        <StackPanel>
                            <Label Margin="0,25,0,0" Content="PRIMARY" FontSize="17" FontWeight="Bold"/>
                            <StackPanel Orientation="Horizontal">
                                <StackPanel>
                                    <Label Content="Name: First" />
                                    <TextBox Text="{Binding CurrentPageData.PrimaryDecedent.FirstName}" 
                                         Margin="2,0,0,2" Width="200" KeyDown="Textbox_KeyDown"/>
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Middle" />
                                    <TextBox Text="{Binding CurrentPageData.PrimaryDecedent.MiddleName}" 
                                         Margin="2,0,0,2" Width="200" KeyDown="Textbox_KeyDown"/>
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Last" />
                                    <TextBox x:Name="primaryLastName" Margin="2,0,0,2" Width="200" KeyDown="Textbox_KeyDown"
                                         Text="{Binding CurrentPageData.PrimaryDecedent.LastName}"/>
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Suffix" />
                                    <TextBox Text="{Binding CurrentPageData.PrimaryDecedent.Suffix}" 
                                         Margin="2,0,0,2"  Width="95" KeyDown="Textbox_KeyDown"/>
                                </StackPanel>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <StackPanel>
                                    <Label Content="Location" />
                                    <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.Location}" 
                                            Margin="2,0,0,2" Width="300" Padding="1" 
                                            HorizontalAlignment="Stretch" VerticalAlignment="Center"
                                            TextSearch.TextPath="Location" IsTextSearchEnabled="True"
                                            ItemTemplate="{StaticResource locationCmbTemplate}"
                                            ItemsSource="{Binding GetLocationData}">
                                    </ComboBox>
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Rank" />
                                    <TextBox Text="{Binding CurrentPageData.PrimaryDecedent.RankList[0]}"
                                             Padding="1"
                                             Margin="2,0,0,2" Width="200" KeyDown="Textbox_KeyDown"/>
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Branch" />
                                    <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.BranchList[0]}" 
                                              Margin="2,0,0,2" Width="200" Padding="1" 
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemTemplate="{StaticResource branchCmbTemplate}"
                                              TextSearch.TextPath="Code" IsTextSearchEnabled="True"
                                              ItemsSource="{Binding GetBranchData}" LostFocus="Branch_LostFocus"/>
                                </StackPanel>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <StackPanel>
                                    <Label Content="Branch-Unit Custom" />
                                    <TextBox Text="{Binding CurrentPageData.PrimaryDecedent.BranchUnitCustom}"  
                                             Margin="2,0,0,2"  Width="702" KeyDown="Textbox_KeyDown"/>
                                </StackPanel>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <StackPanel>
                                    <Label Content="War" />
                                    <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.WarList[0]}" 
                                              Margin="2,0,0,2" Width="100" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemTemplate="{StaticResource warCmbTemplate}"
                                              TextSearch.TextPath="Code" IsTextSearchEnabled="True"
                                              ItemsSource="{Binding GetWarData}" LostFocus="War_LostFocus"/>
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="War 2" />
                                    <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.WarList[1]}" 
                                              Margin="2,0,0,2" Width="100" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemTemplate="{StaticResource warCmbTemplate}"
                                              TextSearch.TextPath="Code" IsTextSearchEnabled="True"
                                              ItemsSource="{Binding GetWarData}" LostFocus="War_LostFocus"/>
                                </StackPanel>
                                <StackPanel>
                                    <Label Width="90"/>
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Birth Date" />
                                    <xctk:MaskedTextBox x:Name="_maskedTextBox"
                                                        Mask="00/00/0000"
                                                        ValueDataType="{x:Type s:String}" 
                                                        Value="{Binding CurrentPageData.PrimaryDecedent.BirthDate}"/>

                                    <!--<TextBox  VerticalAlignment="Center" Margin="2,0,2,0"
                                              Width="200" KeyDown="Textbox_KeyDown"
                                            Text="{Binding CurrentPageData.PrimaryDecedent.BirthDate, StringFormat=MM/dd/yyyy}" />-->
                                </StackPanel>
                                <StackPanel>
                                    <Label Content="Death Date" />
                                    <TextBox Margin="2,0,2,0"  Width="200" KeyDown="Textbox_KeyDown"
                                             PreviewTextInput="NumberValidationTextBox"
                                             Text="{Binding CurrentPageData.PrimaryDecedent.DeathDate, StringFormat=MM/dd/yyyy}"
                                             />
                                </StackPanel>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <StackPanel>
                                    <Label Content="Award" />
                                    <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.AwardList[0]}" 
                                              Margin="2,0,0,2" Width="100" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemTemplate="{StaticResource awardCmbTemplate}"
                                              TextSearch.TextPath="Code" IsTextSearchEnabled="True"
                                              ItemsSource="{Binding GetAwardData}" LostFocus="Award_LostFocus"/>
                                </StackPanel>
                                <StackPanel Grid.Column="1">
                                    <Label Content="Award 2" />
                                    <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.AwardList[1]}" 
                                              Margin="2,0,0,2" Width="100" Padding="1" 
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemTemplate="{StaticResource awardCmbTemplate}"
                                              TextSearch.TextPath="Code" IsTextSearchEnabled="True"
                                              ItemsSource="{Binding GetAwardData}" LostFocus="Award_LostFocus"/>
                                </StackPanel>
                                <StackPanel>
                                    <Label Width="90"/>
                                </StackPanel>
                                <StackPanel Grid.Column="3">
                                    <Label Content="Awards Custom (Enter multiple w/ spaces between)" />
                                    <TextBox KeyDown="Textbox_KeyDown"
                                             Text="{Binding CurrentPageData.PrimaryDecedent.AwardCustom}" 
                                             Margin="2,0,2,0"  Width="405"/>
                                </StackPanel>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <StackPanel>
                                    <Label Content="Inscription" />
                                    <TextBox KeyDown="Textbox_KeyDown"
                                             Text="{Binding CurrentPageData.PrimaryDecedent.Inscription}"
                                             Margin="2,0,0,2"  Width="700">
                                    </TextBox>
                                </StackPanel>
                            </StackPanel>

                            <StackPanel Margin="0,20,0,10">
                                <StackPanel.Resources>
                                    <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
                                </StackPanel.Resources>

                                <CheckBox  Name="morePrimaryData"
                                  Content="Add Additional Information for Primary"/>

                                <StackPanel Orientation="Horizontal" 
                                    Visibility="{Binding ElementName=morePrimaryData, Path=IsChecked, 
                                    Converter={StaticResource BooleanToVisibilityConverter}}">
                                    <StackPanel>
                                        <Label Content="War 3" />
                                        <ComboBox  Text="{Binding CurrentPageData.PrimaryDecedent.WarList[2]}" 
                                               Margin="2,2,0,2" Width="100" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemTemplate="{StaticResource warCmbTemplate}"
                                              TextSearch.TextPath="Code" IsTextSearchEnabled="True"
                                              ItemsSource="{Binding GetWarData}" LostFocus="War_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel>
                                        <Label Content="War 4" />
                                        <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.WarList[3]}" 
                                              Margin="2,2,0,2" Width="100" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemTemplate="{StaticResource warCmbTemplate}"
                                              TextSearch.TextPath="Code" IsTextSearchEnabled="True"
                                              ItemsSource="{Binding GetWarData}" LostFocus="War_LostFocus"/>
                                    </StackPanel>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" 
                                    Visibility="{Binding ElementName=morePrimaryData, Path=IsChecked, 
                                    Converter={StaticResource BooleanToVisibilityConverter}}">
                                    <StackPanel>
                                        <Label Content="Award 3" />
                                        <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.AwardList[2]}" 
                                              Margin="2,0,0,2" Width="90" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemTemplate="{StaticResource awardCmbTemplate}"
                                              TextSearch.TextPath="Code" IsTextSearchEnabled="True"
                                              ItemsSource="{Binding GetAwardData}" LostFocus="Award_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel>
                                        <Label Content="Award 4" />
                                        <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.AwardList[3]}" 
                                              Margin="2,0,0,2" Width="90" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemTemplate="{StaticResource awardCmbTemplate}"
                                              TextSearch.TextPath="Code" IsTextSearchEnabled="True"
                                              ItemsSource="{Binding GetAwardData}" LostFocus="Award_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel>
                                        <Label Content="Award 5" />
                                        <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.AwardList[4]}"  
                                              Margin="2,0,0,2" Width="90" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemTemplate="{StaticResource awardCmbTemplate}"
                                              TextSearch.TextPath="Code" IsTextSearchEnabled="True"
                                              ItemsSource="{Binding GetAwardData}" LostFocus="Award_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel>
                                        <Label Content="Award 6" />
                                        <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.AwardList[5]}" 
                                              Margin="2,0,0,2" Width="90" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemTemplate="{StaticResource awardCmbTemplate}"
                                              TextSearch.TextPath="Code" IsTextSearchEnabled="True"
                                              ItemsSource="{Binding GetAwardData}" LostFocus="Award_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel>
                                        <Label Content="Award 7" />
                                        <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.AwardList[6]}" 
                                              Margin="2,0,0,2" Width="90" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemTemplate="{StaticResource awardCmbTemplate}"
                                              TextSearch.TextPath="Code" IsTextSearchEnabled="True"
                                              ItemsSource="{Binding GetAwardData}" LostFocus="Award_LostFocus"/>
                                    </StackPanel>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" 
                                    Visibility="{Binding ElementName=morePrimaryData, Path=IsChecked, 
                                    Converter={StaticResource BooleanToVisibilityConverter}}">
                                    <StackPanel>
                                        <Label Content="Rank 2" />
                                        <TextBox Text="{Binding CurrentPageData.PrimaryDecedent.RankList[1]}"  
                                              Margin="2,0,0,2" Width="100" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center"/>
                                    </StackPanel>
                                    <StackPanel>
                                        <Label Content="Branch 2" />
                                        <ComboBox Text="{Binding CurrentPageData.PrimaryDecedent.BranchList[1]}"
                                              Margin="2,0,0,2" Width="100" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center" 
                                              ItemTemplate="{StaticResource branchCmbTemplate}"
                                              TextSearch.TextPath="Code" IsTextSearchEnabled="True"
                                              ItemsSource="{Binding GetBranchData}" LostFocus="Branch_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel>
                                        <Label Content="Rank 3" />
                                        <TextBox Text="{Binding CurrentPageData.PrimaryDecedent.RankList[2]}"
                                              Margin="2,0,0,2" Width="100" Padding="1"
                                              HorizontalAlignment="Stretch" VerticalAlignment="Center"/>
                                    </StackPanel>
                                    <StackPanel>
                                        <Label Content="Branch 3" />
                                        <ComboBox  Text="{Binding CurrentPageData.PrimaryDecedent.BranchList[2]}"
                                               Margin="2,0,0,2" Width="100" Padding="1"
