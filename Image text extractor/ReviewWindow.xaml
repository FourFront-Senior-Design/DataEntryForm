                                            <TextBox Text="{Binding CurrentPageData.OthersDecedentList[5].DeathDate, StringFormat=MM/dd/yyyy}"
                                            Margin="2,0,2,50"  Width="350" KeyDown="Textbox_KeyDown"/>
